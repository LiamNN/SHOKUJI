<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>
<div class="container p-5">
<% @recipes.each do |recipe| %>
  <%= link_to recipe_path(recipe), class:'text-decoration-none' do %>
    <h4><%= recipe.name %></h4>
    <p style='color: #476072;'><%= recipe.meal_time %></p>
  <% end %>
<% end %>
</div>
